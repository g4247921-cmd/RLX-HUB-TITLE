-- Obfuscated + encoded texts (fixed & working)
local P=game:GetService("Players")
local ME=P.LocalPlayer
local PG=ME:WaitForChild("PlayerGui")

-- decoder: prefix (emoji) + reverse(suffixEncoded)
local function d(pref,enc) return (pref or "") .. string.reverse(enc or "") end

-- encoded (reversed) teks
local T1 = d("üå≤ ","XLR")                           -- üå≤ RLX
local T2 = d("üå≤ ","BUH RETSAM XLR")                -- üå≤ RLX MASTER HUB
local T3 = d("üéñÔ∏è ","reviG eltiT")                  -- üéñÔ∏è Title Giver
local T4 = d("‚úÖ ","eltiT gnasaP")                  -- ‚úÖ Pasang Title
local T5 = d("‚ôªÔ∏è ","eltiT teseR")                   -- ‚ôªÔ∏è Reset Title
local T6 = d("","...umak eltit kiteK")             -- Ketik title kamu...
local T7 = d(""," :ijomE hiliP")                   -- Pilih Emoji:
local T8 = d(""," :anraW hiliP")                   -- Pilih Warna:
local T9 = d("","reyalP")                          -- Player
local T10 = d("‚úÖ ","!gnasapid lisahreb eltiT")     -- ‚úÖ Title berhasil dipasang!
local T11 = d("‚ôªÔ∏è ","!teserid eltiT")               -- ‚ôªÔ∏è Title direset!
local T12 = d("","!yllufsseccuS dedaoL IU reviG eltiT XLR") -- load msg

-- bersihkan GUI lama
if PG:FindFirstChild("RLX_TitleGiver_Full") then
	pcall(function() PG.RLX_TitleGiver_Full:Destroy() end)
end

-- mulai build (variabel obfuscated)
local a=Instance.new("ScreenGui")
a.Name="RLX_TitleGiver_Full"
a.ResetOnSpawn=false
a.ZIndexBehavior=Enum.ZIndexBehavior.Sibling
a.Parent=PG

local b=Instance.new("Frame",a)
b.Size=UDim2.new(1,0,1,0)
b.BackgroundColor3=Color3.fromRGB(5,40,10)
b.ZIndex=50

local c=Instance.new("ImageButton",b)
c.Name="LogoButton"
c.AnchorPoint=Vector2.new(0.5,0.5)
c.Size=UDim2.new(0.5,0,0.5,0)
c.Position=UDim2.new(0.5,0,0.45,0)
c.Image="rbxassetid://109697835942498"
c.BackgroundTransparency=1
c.ImageTransparency=1
c.ZIndex=51
c.AutoButtonColor=false
c.ScaleType=Enum.ScaleType.Fit

-- fade function (fixed)
local function fade(img,from,to,steps,waitTime)
	steps = tonumber(steps) or 10
	waitTime = tonumber(waitTime) or 0.03
	for i=0,steps do
		local t = from + (to - from) * (i / steps)
		pcall(function() img.ImageTransparency = t end)
		task.wait(waitTime)
	end
end

local splashDone=false

local function finishSplash()
	if splashDone then return end
	splashDone=true
	for i=1,12 do
		pcall(function() b.BackgroundTransparency = i*(1/12) end)
		task.wait(0.03)
	end
	pcall(function() b:Destroy() end)
	-- pastikan hub muncul setelah splash hilang
	if hubFrame then
		pcall(function() hubFrame.Visible = true end)
	end
end

-- spawn fade & auto-skip
task.spawn(function()
	fade(c,1,0,12,0.03)
	local t=0
	while t<2 do
		if splashDone then return end
		task.wait(0.2)
		t+=0.2
	end
	if not splashDone then finishSplash() end
end)

c.MouseButton1Click:Connect(finishSplash)

-- HUB button + frame
local hubBtn=Instance.new("TextButton",a)
hubBtn.Size=UDim2.new(0,70,0,40)
hubBtn.Position=UDim2.new(0.01,0,0.06,0)
hubBtn.Text=T1
hubBtn.Font=Enum.Font.GothamBold
hubBtn.TextSize=16
hubBtn.BackgroundColor3=Color3.fromRGB(30,90,40)
hubBtn.TextColor3=Color3.new(1,1,1)
hubBtn.ZIndex=40
hubBtn.Active=true
hubBtn.Draggable=true
Instance.new("UICorner",hubBtn).CornerRadius=UDim.new(0,8)

local hubFrame=Instance.new("Frame",a)
hubFrame.Size=UDim2.new(0,320,0,420)
hubFrame.Position=UDim2.new(0.01,0,0.12,0)
hubFrame.BackgroundColor3=Color3.fromRGB(15,60,25)
hubFrame.BorderSizePixel=0
hubFrame.Visible=false
hubFrame.ZIndex=40
Instance.new("UICorner",hubFrame).CornerRadius=UDim.new(0,12)

local hubTitle=Instance.new("TextLabel",hubFrame)
hubTitle.Size=UDim2.new(1,0,0,40)
hubTitle.BackgroundTransparency=1
hubTitle.Font=Enum.Font.GothamBold
