	btn.BackgroundColor3=Color3.fromRGB(45,45,45)
	btn.TextColor3=Color3.new(1,1,1)
	Instance.new("UICorner",btn).CornerRadius=UDim.new(0,6)
	xpos+=42
	btn.MouseButton1Click:Connect(function() inputBox.Text = em .. " " .. inputBox.Text end)
end

-- color picker
local colors = {
	Color3.fromRGB(255,255,255),
	Color3.fromRGB(255,0,0),
	Color3.fromRGB(0,255,0),
	Color3.fromRGB(0,170,255),
	Color3.fromRGB(255,255,0),
	Color3.fromRGB(255,0,255),
	Color3.fromRGB(255,128,0),
}
local chosenColor = Color3.new(1,1,1)

local cLabel=Instance.new("TextLabel",scroll)
cLabel.Size=UDim2.new(1,-20,0,25)
cLabel.Position=UDim2.new(0,10,0,140)
