local cFrame=Instance.new("Frame",scroll)
cFrame.Size=UDim2.new(1,-20,0,40)
cFrame.Position=UDim2.new(0,10,0,165)
cFrame.BackgroundTransparency=1

local cx=0
for _,col in ipairs(colors) do
	local cb=Instance.new("TextButton",cFrame)
	cb.Size=UDim2.new(0,36,0,36)
	cb.Position=UDim2.new(0,cx,0,0)
	cb.BackgroundColor3=col
	cb.Text=""
	Instance.new("UICorner",cb).CornerRadius=UDim.new(0,6)
	cx+=42
	cb.MouseButton1Click:Connect(function() chosenColor=col end)
end

