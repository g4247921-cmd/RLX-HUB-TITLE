local xpos=0
for _,em in ipairs(emojis) do
	local btn=Instance.new("TextButton",eFrame)
	btn.Size=UDim2.new(0,36,0,36)
	btn.Position=UDim2.new(0,xpos,0,0)
	btn.Text=em
	btn.Font=Enum.Font.GothamBold
