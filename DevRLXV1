-- popup
local function popup(txt,col)
	col = col or Color3.new(1,1,1)
	local n=Instance.new("TextLabel",a)
	n.Size=UDim2.new(0,260,0,42)
	n.Position=UDim2.new(0.5,-130,0.05,0)
	n.AnchorPoint=Vector2.new(0.5,0)
	n.BackgroundColor3=Color3.fromRGB(30,30,30)
	n.TextColor3=col
	n.Font=Enum.Font.GothamBold
	n.TextSize=16
	n.Text=txt
	n.ZIndex=60
	Instance.new("UICorner",n).CornerRadius=UDim.new(0,8)
	for i=1,10 do n.TextTransparency=1-i*0.1; n.BackgroundTransparency=1-i*0.1; task.wait(0.02) end
	task.wait(1.2)
	for i=1,10 do n.TextTransparency=i*0.1; n.BackgroundTransparency=i*0.1; task.wait(0.02) end
	pcall(function() n:Destroy() end)
end

-- setTitle
local function setTitle(pl,txt,col)
	local ch = pl.Character or pl.CharacterAdded:Wait()
	local head = ch:FindFirstChild("Head")
	if not head then return end
	local old = head:FindFirstChild("RLX_Title")
	if old then old:Destroy() end
	local bill = Instance.new("BillboardGui",head)
	bill.Name="RLX_Title"
	bill.Size=UDim2.new(0,200,0,50)
	bill.StudsOffset=Vector3.new(0,2.5,0)
	bill.AlwaysOnTop=true
	local lbl = Instance.new("TextLabel",bill)
	lbl.Size=UDim2.new(1,0,1,0)
	lbl.BackgroundTransparency=1
	lbl.Font=Enum.Font.GothamBold
	lbl.TextColor3=col
	lbl.TextStrokeTransparency=0.4
	lbl.TextSize=18
	lbl.Text=txt
end

-- connections
hubBtn.MouseButton1Click:Connect(function() hubFrame.Visible = not hubFrame.Visible end)
titleBtn.MouseButton1Click:Connect(function() titleFrame.Visible = not titleFrame.Visible end)

applyBtn.MouseButton1Click:Connect(function()
	local txt = (inputBox.Text~="") and inputBox.Text or T9
	setTitle(ME, txt, chosenColor)
	popup(T10, Color3.fromRGB(100,255,100))
end)

resetBtn.MouseButton1Click:Connect(function()
	local head = ME.Character and ME.Character:FindFirstChild("Head")
	if head and head:FindFirstChild("RLX_Title") then
		head.RLX_Title:Destroy()
	end
	inputBox.Text=""
	popup(T11, Color3.fromRGB(255,200,200))
end)

c.MouseButton1Click:Connect(function()
	if b and b.Parent then pcall(function() b:Destroy() end) end
	hubFrame.Visible = true
end)

print(T12)
